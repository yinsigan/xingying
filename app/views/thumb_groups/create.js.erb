<% if @thumb_group.errors.any? %>
  $(".alert-warning.alert-fixed-top").html("<%= j(render partial: 'shared/ajax_error_messages') %>").fadeIn().delay(5000).fadeOut();
<% else %>
  $(".alert-success.alert-fixed-top").html("<%= j(I18n.t('success_submit')) %>").fadeIn().delay(2000).fadeOut();
  $('#modal').delay(2000).modal('hide');
  $("#group").append("<%= j render partial: "thumb_groups/thumb_group", locals: {thumb_group: @thumb_group} %>")
<% end %>