%h1 Listing page_requests

%table
  %tr
    %th Path
    %th Page duration
    %th View duration
    %th Db duration
    %th Index
    %th
    %th
    %th

  - @page_requests.each do |page_request|
    %tr
      %td= page_request.path
      %td= page_request.page_duration
      %td= page_request.view_duration
      %td= page_request.db_duration
      %td= page_request.index
      %td= link_to 'Show', page_request
      %td= link_to 'Edit', edit_page_request_path(page_request)
      %td= link_to 'Destroy', page_request, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Page request', new_page_request_path
